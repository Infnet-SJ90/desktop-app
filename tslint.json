{
	"defaultSeverity": "warning",
	"extends": [
		"tslint:recommended"
	],
	"linterOptions": {
		"exclude": [
			"node_modules/**"
		]
	},
	"rules": {
		"indent": [true, "tabs", 4],
		"interface-name": false,
		"no-consecutive-blank-lines": false,
		"one-line": false,
		"ordered-imports": false,
		"no-trailing-whitespace": false,
		"max-line-length": false,
		"prefer-for-of": false
	}
}
