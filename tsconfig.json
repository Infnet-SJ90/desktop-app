{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"strict": true,
		"strictNullChecks": false,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*"]
		}
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
	  "node_modules"
	]
}
