{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es2015",
		"moduleResolution": "node",
		"strict": true,
		"strictNullChecks": false,
		"noImplicitAny": true,
        "pretty": true,
        "newLine": "LF",
        "allowSyntheticDefaultImports": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "allowJs": true,
		"outDir": "./dist/",
		"baseUrl": "."
	},
	"exclude": [
	  "node_modules"
	]
}
